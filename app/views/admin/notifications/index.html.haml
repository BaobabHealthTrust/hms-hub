:ruby
  @nav_hierarchy = [:notifications]

%form.search
  = text_field_tag :phone_number, params[:phone_number], :placeholder => t('.search_placeholder')

- if @phone_number.present?
  %h2= t('.search_title', :phone_number => @phone_number)
- else
  %h2= t('.title')

#paginator= paginate @notifications
= render 'table', :notifications => @notifications
